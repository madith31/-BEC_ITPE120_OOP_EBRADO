class MyClass {
       static int instanceCount = 0;
    
    MyClass() {
              instanceCount++;
    }
    

    public static void main(String[] args) {
        MyClass obj1 = new MyClass();
        MyClass obj2 = new MyClass();
        MyClass obj3 = new MyClass();
        
   System.out.println("Number of instances: " + MyClass.instanceCount);
    }
}

